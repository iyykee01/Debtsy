import useRequest from "../../../../helpers/request";

class ConfirmOtp {
  //   async getUsers(page?: number, perPage?: number) {
  //     return await pocketbase.collection("users").getList(page, perPage);
  //   }

  async confirmOtp(data: any) {
    return await useRequest.post({ path: "/auth/signup", data });
  }
}

const OtpService = new ConfirmOtp();

export default OtpService;
